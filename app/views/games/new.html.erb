<h2>Random letter</h2>
<p id= "randomLetters"><%= @letters %></p>

<form action="/score" method="post">
  <input type="text" name="word">
  <input type="submit">
  <input id="token" name="randomLetters" type="hidden" value="<%= @letters %>" />
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
</form>
